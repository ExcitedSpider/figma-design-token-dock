  "description": "Your Figma Plugin",
  "main": "code.js",
  "scripts": {
    "build": "webpack --mode=production",
    "start": "webpack --mode=development --watch",
    "lint:fix": "eslint --ext .ts .js .tsx .jsx --fix src/** --no-error-on-unmatched-pattern",
    "lint": "eslint --ext .ts .js .tsx .jsx src/** --no-error-on-unmatched-pattern",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint:css": "stylelint **/*.{html,vue,css,sass,scss,less}"
  },
  "lint-staged": {
    "*.{html,vue,css,sass,scss,less}": [
      "stylelint --fix",
      "git add"
    ]
  },
